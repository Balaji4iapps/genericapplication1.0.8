{
  "description": "",
  "root": "callFunctionCalculation",
  "actions": {
    "callFunctionCalculation": {
      "module": "vb/action/builtin/callModuleFunctionAction",
      "parameters": {
        "module": "[[ $functions ]]",
        "functionName": "calculation",
        "params": [
          "{{ $variables.index }}"
        ]
      }
    }
  },
  "variables": {
    "value": {
      "required": true,
      "type": "any",
      "input": "fromCaller"
    },
    "key": {
      "required": true,
      "type": "any",
      "input": "fromCaller"
    },
    "index": {
      "required": true,
      "type": "number",
      "input": "fromCaller"
    },
    "current": {
      "required": true,
      "type": "any",
      "input": "fromCaller"
    }
  }
}