{
  "title": "Create Department",
  "description": "Creates new Department",
  "variables": {
    "createDepartmentChainInProgress": {
      "type": "boolean",
      "defaultValue": false
    },
    "department": {
      "type": "page:createDepartmentRequest"
    }
  },
  "metadata": {},
  "types": {
    "@decorators.createDepartmentRequest": {
      "endpoint": "create_Department",
      "service": "businessObjects",
      "path": "request"
    },
    "businessObjectsServiceErrorResponseType": {
      "body": {
        "status": "string",
        "title": "string",
        "o:errorDetails": [
          {
            "detail": "string",
            "o:errorCode": "string"
          }
        ]
      },
      "status": "number"
    },
    "createDepartmentRequest": {
      "deptCode": "string",
      "deptName": "string",
      "salary": "number"
    }
  },
  "eventListeners": {
    "backButtonClicked": {
      "chains": [
        {
          "chainId": "goBackChain"
        }
      ]
    },
    "saveButtonClicked": {
      "chains": [
        {
          "chainId": "createDepartmentChain"
        }
      ]
    },
    "toggleDrawer": {
      "chains": [
        {
          "chainId": "application:toggleDrawerHandler"
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-form-layout": {
        "path": "ojs/ojformlayout"
      },
      "oj-input-number": {
        "path": "ojs/ojinputnumber"
      },
      "oj-input-text": {
        "path": "ojs/ojinputtext"
      },
      "oj-toolbar": {
        "path": "ojs/ojtoolbar"
      },
      "oj-validation-group": {
        "path": "ojs/ojvalidationgroup"
      }
    }
  }
}