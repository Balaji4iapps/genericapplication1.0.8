{
  "title": "barcodescanner",
  "description": "",
  "variables": {
    "value": {
      "type": "string"
    }
  },
  "metadata": {},
  "types": {},
  "eventListeners": {
    "buttonAction": {
      "chains": [
        {
          "chainId": "oraclebtn"
        }
      ]
    },
    "buttonAction2": {
      "chains": [
        {
          "chainId": "fusionbtn"
        }
      ]
    },
    "buttonClick": {
      "chains": [
        {
          "chainId": "CameraFilePickerSelectChain"
        }
      ]
    },
    "cameraFilePickerSelect": {
      "chains": [
        {
          "chainId": "CameraFilePickerSelectChain",
          "parameters": {
            "files": "[[ Array.prototype.slice.call($event.detail.files) ]]"
          }
        }
      ]
    },
    "toggleDrawer": {
      "chains": [
        {
          "chainId": "application:toggleDrawerHandler"
        }
      ]
    },
    "vbEnter": {
      "chains": [
        {
          "parameters": {},
          "chainId": "onload"
        }
      ]
    }

  },
  "imports": {
    "components": {
      "oj-button": {
        "path": "ojs/ojbutton"
      },
      "oj-file-picker": {
        "path": "ojs/ojfilepicker"
      },
      "oj-sample-qr-code": {
        "path": "oj-sample/qr-code/loader"
      }
    }
  }
}